#include <bits/stdc++.h>

int main(){
    int n;
    scanf("%i", &n);
    int wynik=0;

    for (int i=1; i<n; i++){

        if (n%i==0){
            wynik+=i;
        }
        
    }
    if (wynik==n){
        printf("Jest doskonala");
    }

    else printf("Nie jest doskonala");

    return 0;
}
